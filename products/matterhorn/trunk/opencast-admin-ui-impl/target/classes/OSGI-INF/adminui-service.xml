<?xml version="1.0" encoding="UTF-8"?>
<component name="org.opencastproject.adminui.api.AdminuiService" immediate="true">
  <implementation class="org.opencastproject.adminui.impl.AdminuiServiceImpl" />
  <property name="service.description" value="Adminui Service" />
  <service>
    <provide interface="org.opencastproject.adminui.api.AdminuiService" />
    <provide interface="org.osgi.service.cm.ManagedService" />
  </service>
  <reference name="scheduler-service" interface="org.opencastproject.scheduler.api.SchedulerService"
    cardinality="0..1" policy="dynamic" bind="setSchedulerService" unbind="unsetSchedulerService" />
  <reference name="workflow-service" interface="org.opencastproject.workflow.api.WorkflowService"
    cardinality="0..1" policy="dynamic" bind="setWorkflowService" unbind="unsetWorkflowService" />
  <reference name="capture-admin-service" interface="org.opencastproject.capture.admin.api.CaptureAgentStatusService"
    cardinality="0..1" policy="dynamic" bind="setCaptureAdminService" unbind="unsetCaptureAdminService" />
</component>
