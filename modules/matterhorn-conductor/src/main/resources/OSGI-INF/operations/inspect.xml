    <provide interface="org.opencastproject.workflow.api.WorkflowOperationHandler" />
  </service>
  <reference cardinality="1..1" interface="org.opencastproject.inspection.api.MediaInspectionService"
  name="InspectionService" policy="static" bind="setInspectionService"/>
  <reference name="dc" interface="org.opencastproject.metadata.dublincore.DublinCoreCatalogService"
    cardinality="1..1" policy="static" bind="setDublincoreService" />
  <reference cardinality="1..1" interface="org.opencastproject.workspace.api.Workspace"
	name="Workspace" policy="static" bind="setWorkspace"/>
  <reference name="metadata" interface="org.opencastproject.metadata.api.MediaPackageMetadataService"
    cardinality="0..n" bind="addMetadataService" unbind="removeMetadataService" />
</component>
