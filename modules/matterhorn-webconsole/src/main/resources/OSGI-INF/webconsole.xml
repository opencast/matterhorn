<?xml version="1.0" encoding="UTF-8"?>
<component name="org.opencastproject.webconsole" immediate="true"
  activate="activate" deactivate="deactivate">
  <implementation class="org.opencastproject.webconsole.Component" />
  <property name="service.description" value="Matterhorn OSGI management console" />
  <service>
    <provide interface="org.opencastproject.webconsole.Component" />
  </service>
  <reference name="http-service" interface="org.osgi.service.http.HttpService"
    cardinality="1..1" policy="static" bind="setHttpService" />
  <reference name="http-context" interface="org.osgi.service.http.HttpContext"
    cardinality="1..1" policy="static" bind="setHttpContext" />
</component>
