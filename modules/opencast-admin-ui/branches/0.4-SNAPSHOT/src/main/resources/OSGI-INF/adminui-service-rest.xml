<?xml version="1.0" encoding="UTF-8"?>
<component name="org.opencastproject.adminui.endpoint.AdminuiRestService" immediate="true">
  <implementation class="org.opencastproject.adminui.endpoint.AdminuiRestService" />
  <property name="service.description" value="Admin UI REST Proxy" />
  <property name="service.exported.interfaces" value="org.opencastproject.adminui.endpoint.AdminuiRestService" />
  <property name="service.exported.configs" value="org.apache.cxf.rs" />
  <property name="org.apache.cxf.rs.httpservice.context" value="/admin/rest" />
  <service>
    <provide interface="org.opencastproject.adminui.endpoint.AdminuiRestService" />
  </service>
  <reference name="workflow-service" interface="org.opencastproject.workflow.api.WorkflowService"
    cardinality="0..1" policy="dynamic" bind="setWorkflowService" unbind="unsetWorkflowService" />
  <reference name="scheduler-service" interface="org.opencastproject.scheduler.api.SchedulerService"
    cardinality="0..1" policy="dynamic" bind="setSchedulerService" unbind="unsetSchedulerService" />
  <reference name="capture-service" interface="org.opencastproject.capture.admin.api.CaptureAgentStateService"
    cardinality="0..1" policy="dynamic" bind="setCaptureAdminService" unbind="unsetCaptureAdminService" />
</component>
